#include <stdio.h>

void ratinho()
{
  printf("\n");
  printf("<:3)~~~\n");
  printf("\n");
}
void gatinho()
{
  printf("=^..^=\n");
  printf("\n");
}
void rat達o()
{
  printf(" _   _\n");
  printf("(_)_(_)\n");
  printf(" (o o)\n");
  printf("==\\o/==\n");
  printf("\n");
}
void gat達o()
{
  printf(" /\\_/\\\n");
  printf("( o.o )\n");
  printf(" > ^ <\n");
  printf("\n");
}
int main()
{
  printf("Oi,\n");
  ratinho();
  gatinho();
  rat達o();
  gat達o();
  printf("mundo\n");
}

